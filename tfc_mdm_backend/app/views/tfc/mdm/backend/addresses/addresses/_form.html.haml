= form.input :record, as: :polymorphic_association, classes: { Tfc::Mdm::Club => url_for([:autocomplete, Tfc::Mdm::Club]), Tfc::Mdm::Person => url_for([:autocomplete, Tfc::Mdm::Person]) }

= form.association :role, label_method: :identifier

= form.input :valid_from, as: :string, input_html: { type: :date }
= form.input :valid_to, as: :string, input_html: { type: :date }

= form.input :line1
= form.input :line2
= form.input :street
= form.input :zipcode
= form.input :city
= form.input :country
